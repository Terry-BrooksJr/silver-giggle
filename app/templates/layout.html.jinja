    <link rel="stylesheet" type="text/css" href="{{ url_for( 'static', filename='./css/base_style.css' ) }}">

    {% if template == 'login'%}
    <link rel="stylesheet" type="text/css" href="{{ url_for( 'static', filename='./css/login_style.css' ) }}"> 
    {% elif template == 'base' %}
    <link rel="stylesheet" type="text/css" href="{{ url_for( 'static', filename='/app/static/css/landing_page_style.css') }}"> 
    {% elif template == '404' %}
    <link rel="stylesheet" type="text/css" href="{{ url_for( 'static', filename='/app/static/css/404_.css') }}"> 
    {% elif template == 403 %}
    <link rel="stylesheet" type="text/css" href="{{ url_for( 'static', filename='/app/static/css/403_.css') }}"> 
    {% endif %}

</head>

<body>
